{
  "permissions": {
    "allow": [
      "mcp__laravel-boost__database-query",
      "mcp__laravel-boost__application-info",
      "Bash(php artisan make:migration:*)",
      "Bash(php artisan make:model:*)",
      "Bash(php artisan make:factory:*)",
      "mcp__laravel-boost__get-config",
      "Bash(php artisan migrate:*)",
      "mcp__laravel-boost__database-schema",
      "Bash(php artisan make:policy:*)",
      "Bash(php artisan make:livewire:*)",
      "Bash(php artisan migrate:fresh:*)",
      "Bash(php artisan config:clear:*)",
      "Bash(php artisan:*)",
      "Bash(php artisan view:clear:*)",
      "Bash(npm run build:*)",
      "Bash(ln:*)",
      "mcp__laravel-boost__last-error",
      "mcp__laravel-boost__search-docs",
      "Bash(composer:*)",
      "Bash(vendor/bin/pint:*)",
      "Bash(git checkout:*)",
      "mcp__laravel-boost__browser-logs",
      "mcp__laravel-boost__tinker",
